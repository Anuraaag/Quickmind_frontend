{
    "manifest_version": 3,
    "name": "Quickmind: An Assistant on Steroids",
    "version": "2.0.0",
    "description": "A super advanced chatbot that will assist you with anything, and will become your go-to source for accurate and instant answers.",
    "action": {
        "default_title": "Quickmind: An Assistant on Steroids",
        "default_popup": "popup.html"
    },
    "content_security_policy": {
        "script-src": "self",
        "object-src": "self"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "botScript.js"
            ],
            "matches": [],
            "extension_ids": []
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}